// 将设置放入此文件中以覆盖默认值和用户设置。
{
    "emeraldwalk.runonsave": {
        "autoClearConsole": true,
        "commands": [
            {
                "match": "\\.tsx?$",
                "isAsync": true,
                "cmd": "tsc -p ${workspaceRoot} && echo Running on save finished."
            }
        ]
    }
}
